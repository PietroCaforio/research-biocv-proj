{
    "wandb": {
        "project_name": "bestmodels_features",
        "name": "CPTACPDA_nobackbone"
    },
    "data_training": {
        "ct_path": "../MedImageInsights/embeddings_output_cptacpda_93",
        "wsi_path": "../../trident_processed_PDA_univ2/20x_256px_0px_overlap/features_uni_v2/",
        "labels_splits_path": "./data/processed/processed_CPTAC_PDA_survival/k=all.tsv",
        "missing_modality_prob":0.30,
        "require_both_modalities":true,
        "pairing_mode":"one_to_one",
        "allow_repeats":true,
        "pairs_per_patient":null,
        "missing_modality":"both"
    },
    "data_loader": {
        "num_workers": 2
    },
    "model": {
        "checkpoint_path": "./models/ckpts/",
        "rad_input_dim": 1024,
        "histo_input_dim": 1536,
        "inter_dim":256,
        "token_dim":256,
        "dim_hider":256
    },
    "training": {
        "num_epochs": 300,
        "batch_size": 16,
        "learning_rate": 5e-3,
        "pretrained_rad_path":"./models/pretrain_weights/r3d34_K_200ep.pth",
        "pretrained_histo_path":"./models/pretrain_weights/r3d34_K_200ep.pth",
        "checkpoint_dir": "./models/ckpts/",
        "log_interval": 1,
        "scheduler": {
            "type": "cosine_annealing_lr",
            "T_max": 15,
            "eta_min": 9e-5,
            "last_epoch": -1
        },
        "early_stopping": {
            "patience":1000,
            "min_delta":1e-6,
            "mode": "max"
	    },
        "monitor_metric": "val_cindex",
        "gpu":0,
        "n_validations": 15
    }
}
