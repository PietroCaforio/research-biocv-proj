     "shell.execute_reply": "2024-10-13T04:22:05.512511Z"
    },
    "papermill": {
     "duration": 0.025627,
     "end_time": "2024-10-13T04:22:05.515083",
     "exception": false,
     "start_time": "2024-10-13T04:22:05.489456",
     "status": "completed"
    },
